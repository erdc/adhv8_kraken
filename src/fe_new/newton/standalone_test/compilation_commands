#!/bin/bash
mpicc -o e.o -c -fPIC -Wall -Wwrite-strings -Wno-unknown-pragmas -fstack-protector -fno-stack-check -Qunused-arguments -fvisibility=hidden -g -O3 -pedantic -std=c99 -I/opt/homebrew/Cellar/petsc/3.20.5_1/include    `pwd`/e.c
mpicc -Wl,-ld_classic -Wl,-bind_at_load -Wl,-commons,use_dylibs -Wl,-search_paths_first -Wl,-no_compact_unwind  -fPIC -Wall -Wwrite-strings -Wno-unknown-pragmas -fstack-protector -fno-stack-check -Qunused-arguments -fvisibility=hidden -g -O3 -pedantic -std=c99 -o e e.o -Wl,-rpath,/opt/homebrew/Cellar/petsc/3.20.5_1/lib -L/opt/homebrew/Cellar/petsc/3.20.5_1/lib -Wl,-rpath,/opt/homebrew/Cellar/open-mpi/5.0.3_1/lib -L/opt/homebrew/Cellar/open-mpi/5.0.3_1/lib -Wl,-rpath,/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current/gcc/aarch64-apple-darwin23/13 -L/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current/gcc/aarch64-apple-darwin23/14 -Wl,-rpath,/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current/gcc -L/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current/gcc -Wl,-rpath,/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current -L/opt/homebrew/Cellar/gcc/14.1.0_2/lib/gcc/current -lpetsc -llapack -lblas -lmpi_usempif08 -lmpi_usempi_ignore_tkr -lmpi_mpifh -lmpi -lgfortran -ld_classic -lgfortran -lemutls_w -lquadmath -lc++